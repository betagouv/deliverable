{
  "name": "deliverable",
  "description": "CLI tool to generate your project deliverables from your Github repositories.",
  "version": "1.2.0",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "bundle": "rm -Rf ./dist && yarn build && rollup --config ./config/rollup.config.js",
    "build": "rm -Rf ./build && tsc",
    "dev": "chokidar \"./src/**/*.ts\" -c \"yarn build && node ./build/index.js\" -d 1000 --initial --silent",
    "dist:": "rm -Rf ./dist && yarn build && rollup --config ./config/rollup.config.js && yarn build && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable -t node18-linux-x64",
    "dist:linux": "yarn bundle && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable-linux-x64 -t node18-linux-x64 && chmod +x ./dist/deliverable-linux-x64",
    "dist:macos": "yarn bundle && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable-macos-x64 -t node18-macos-x64 && chmod +x ./dist/deliverable-macos-x64",
    "dist:windows": "yarn bundle && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable-windows-x64 -t node18-windows-x64",
    "release": "semantic-release",
    "release:preview": "semantic-release -d",
    "start": "yarn build && node ./build/index.js",
    "setup": "sh ./scripts/setup.sh",
    "test:lint": "eslint --ext js,jsx,ts,tsx .",
    "test:type": "tsc --noEmit",
    "test:unit": "jest --config ./config/jest.config.ts",
    "test:unit:watch": "yarn test --watch"
  },
  "dependencies": {
    "@inquirer/prompts": "2.1.1",
    "@octokit/auth-token": "4.0.0",
    "@octokit/rest": "19.0.13",
    "@vitalets/google-translate-api": "9.2.0",
    "dayjs": "1.11.8",
    "dotenv": "16.3.1",
    "execa": "7.1.1",
    "ora": "6.3.1",
    "which": "3.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@ivangabriele/eslint-config-typescript-base": "7.1.0",
    "@jest/globals": "29.5.0",
    "@rollup/plugin-commonjs": "25.0.2",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@rollup/plugin-replace": "5.0.2",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "6.0.3",
    "@swc/core": "1.3.66",
    "@swc/jest": "0.2.26",
    "@tsconfig/node18": "1.0.1",
    "@types/node": "20.3.1",
    "@types/which": "3.0.0",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "chokidar-cli": "3.0.0",
    "eslint": "8.43.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest": "27.2.2",
    "eslint-plugin-no-null": "1.0.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "eslint-plugin-typescript-sort-keys": "2.3.0",
    "husky": "8.0.3",
    "jest": "29.5.0",
    "lint-staged": "13.2.2",
    "pkg": "5.8.1",
    "prettier": "2.8.8",
    "rollup": "3.25.1",
    "semantic-release": "21.0.5",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "//": "https://github.com/okonet/lint-staged/issues/825#issuecomment-674575655",
  "lint-staged": {
    "*.{json,md,yaml,yml}": "prettier --write",
    "*.ts": [
      "yarn eslint --ext js,ts",
      "bash -c 'npm run test:type'"
    ]
  },
  "packageManager": "yarn@3.3.0"
}
