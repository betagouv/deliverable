{
  "name": "deliverable",
  "description": "CLI tool to generate your project deliverables your Github repositories.",
  "version": "0.0.0",
  "license": "AGPL-3.0-or-later",
  "type": "module",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "bundle": "rm -Rf ./dist && yarn build && rollup --config ./config/rollup.config.js",
    "build:dev": "rm -Rf ./build && tsc",
    "dev": "chokidar \"./src/**/*.ts\" -c \"yarn build && node ./build/index.js\" -d 1000 --initial --silent",
    "dist:": "rm -Rf ./dist && yarn build && rollup --config ./config/rollup.config.js && yarn build && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable -t node18-linux-x64",
    "dist:debian": "yarn bundle && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable -t node18-linux-x64",
    "dist:macos": "yarn bundle && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable -t node18-macos-x64",
    "dist:windows": "yarn bundle && pkg ./dist/deliverable.cjs -d -o ./dist/deliverable -t node18-windows-x64",
    "start": "yarn build && node ./build/index.js",
    "setup": "sh ./scripts/setup.sh",
    "test": "jest --config ./config/jest.config.ts",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@inquirer/prompts": "2.1.1",
    "@octokit/auth-token": "4.0.0",
    "@octokit/rest": "19.0.13",
    "@vitalets/google-translate-api": "9.2.0",
    "dayjs": "1.11.8",
    "ramda": "0.29.0"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "@rollup/plugin-commonjs": "25.0.2",
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-node-resolve": "15.1.0",
    "@swc/core": "1.3.66",
    "@swc/jest": "0.2.26",
    "@tsconfig/node18": "1.0.1",
    "@types/node": "20.3.1",
    "@types/ramda": "0.29.2",
    "chokidar-cli": "3.0.0",
    "dotenv": "16.3.1",
    "jest": "29.5.0",
    "pkg": "5.8.1",
    "prettier": "2.8.8",
    "rollup": "3.25.1",
    "ts-node": "10.9.1",
    "typescript": "5.1.3"
  },
  "packageManager": "yarn@3.3.0"
}
